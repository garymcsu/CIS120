int red = 10;
int yellow = 11;
int green = 12;
int blue = 13;
const int buttonPin = 9;
// second set of lights
int rred = 1;
int yyellow = 2;
int ggreen = 3;
int bblue = 4;
const int bbuttonPin =7;
void setup() {
  pinMode(red, OUTPUT);
  pinMode(yellow, OUTPUT); 
  pinMode(green, OUTPUT);
  pinMode(blue, OUTPUT);
  pinMode(buttonPin, INPUT_PULLUP);

  pinMode(rred, OUTPUT);
  pinMode(yyellow, OUTPUT);
  pinMode(ggreen, OUTPUT);
  pinMode(bblue, OUTPUT);
  pinMode(bbuttonPin, INPUT_PULLUP);
  }
void loop(){
    changeLights();
    delay(5000); //15000
}
void changeLights(){
  if ((digitalRead(buttonPin) == HIGH)) {
    digitalWrite(blue, HIGH);
  } else {
  digitalWrite(blue, LOW);  }
    digitalWrite(green, HIGH);
  digitalWrite(yellow, LOW);
  digitalWrite(red, LOW);
  digitalWrite(bblue, LOW);
  digitalWrite(ggreen, LOW);
  digitalWrite(yyellow, LOW);
    digitalWrite(rred, HIGH);
delay(5000);
   digitalWrite(yellow, HIGH);
  digitalWrite(green, LOW);
  digitalWrite(blue, LOW);
  digitalWrite(red, LOW);
    delay(5000);
digitalWrite(blue, LOW);
  digitalWrite(green, LOW);
  digitalWrite(yellow, LOW);
    digitalWrite(red, HIGH);
 if ((digitalRead(bbuttonPin) == HIGH)) {
    digitalWrite(bblue, HIGH);
  } else {
  digitalWrite(bblue, LOW);  }
    digitalWrite(ggreen, HIGH);
  digitalWrite(yyellow, LOW);
  digitalWrite(rred, LOW);
delay(5000);
digitalWrite(blue, LOW);
  digitalWrite(bblue, LOW);
  digitalWrite(ggreen, LOW);
  digitalWrite(yyellow, HIGH);
  digitalWrite(rred, LOW);
}
